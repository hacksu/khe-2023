{
  "name": "khe-2023",
  "version": "1.0.21",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "templates/*",
    "projects/*",
    "library/*",
    ".github/scripts"
  ],
  "scripts": {
    "dev": "cross-env FORCE_COLOR=1 turbo run dev --parallel --filter=!./templates/*",
    "start": "cross-env FORCE_COLOR=1 turbo run start --parallel --filter=!./templates/*",
    "build": "cross-env FORCE_COLOR=1 turbo run build --parallel --filter=!./templates/*",
    "lint": "cross-env FORCE_COLOR=1 turbo run lint --filter=!./templates/*",
    "postversion": "node .github/scripts/npm/postversion",
    "template": "cd templates && npm run",
    "deploy": "npm version patch",
    "postdeploy": "git commit --allow-empty -m '@deploy' && git push",
    "exports": "exports"
  },
  "devDependencies": {
    "@cseitz/exports": "^1.0.8",
    "@kenthackenough/yarn-scripts": "*",
    "cross-env": "^7.0.3",
    "turbo": "latest"
  },
  "packageManager": "npm@8.19.3",
  "engines": {
    "npm": ">=7.0.0",
    "turbo": ">=14.0.0"
  }
}
